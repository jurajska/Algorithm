#include <math.h>
#include <iostream>

using namespace std;

bool checkNumber(unsigned int n)
{
	if (n <= 1)
		return false;
	if (n == 2)
		return true;
	for (unsigned int i = 2; i <= sqrt(n); ++i)
		if (n % i == 0)
			return false;
	return true;
}


int main()
{
	int n;
	cin >> n;
	unsigned int number;
	
	for (int i = 0; i < n; i++) {
		cin >> number;
		if (checkNumber(number) == true)
		{
			cout << "TAK" << endl;
		}
		else
			cout << "NIE" << endl;
	}
	

	return 0;
}
